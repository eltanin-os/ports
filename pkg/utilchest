[vars]
name=utilchest
version=master
license=UNLICENSE
description=A set of unix utilities written to be simple and small.

[mirrors]
git://github.com/eltanin-os/utilchest.git

[mdeps]
libc

[build]
portsys_mk_meth make replace
portsys_mk_exec utilchest

[install]
portsys_mk_exec install-utilchest
portsys_mk_exec install-man
