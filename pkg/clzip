[vars]
name=clzip
version=1.11
license=GPL-2+
description=A lossless data compressor.
cksum=c124e812e65b23869543c734ea6e606e57d4ad9ee203d1d5a3ee191610dff59b4bdb8c595366e70e8404f440310e56daf7c8e2ef43ade7a7a104a1ae6822abfc

[mirrors]
https://download.savannah.gnu.org/releases/lzip/clzip/clzip-$version.tar.lz

[mdeps]
libc

[build]
mkmeth autohell
mksetflags CC=$"CC CFLAGS=$"CFLAGS CPPFLAGS=$"CPPFLAGS LDFLAGS=$"LDFLAGS
mkexec

[install]
mkexec install
