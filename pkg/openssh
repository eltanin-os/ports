[vars]
name=openssh
version=8.1p1
license=BSD-2 ISC
description=Open Source Secure shell client and server.
cksum=0d78d9e2757d61932a493dea8484364a0c7b6421019bd1feee3f09e865d052078030294b0c7ab58216443e9d3e33564db87b854af729c77c169491bdb8a239eb

[mirrors]
https://cdn.openbsd.org/pub/OpenBSD/OpenSSH/portable/openssh-$version.tar.gz

[mdeps]
libc
libressl
libz

[build]
portsys_mk_meth autohell
portsys_mk_setflags --with-mantype=man
portsys_mk_exec

[install]
portsys_mk_exec install-nokeys
