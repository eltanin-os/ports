[vars]
name=openssh
version=8.1p1
license=BSD-2 ISC
description=Open Source Secure shell client and server.
cksum=0bed9f0531cebd828591bc5017be21c85a3ba144e639f783127bf7f5171b2e7fc53752664dcdde9cc08846a751b243c074038de4a4b8b50e562babdf57a1260d

